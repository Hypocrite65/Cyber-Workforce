{
    "default_model": "qwen_max",
    "models": {
        "qwen_max": {
            "provider": "dashscope",
            "model": "qwen-max",
            "api_key": "your-api-key-here",
            "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1"
        },
        "qwen_turbo": {
            "provider": "dashscope",
            "model": "qwen-turbo",
            "api_key": "your-api-key-here",
            "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1"
        },
        "local_llama": {
            "provider": "ollama",
            "model": "llama3:8b",
            "api_key": "not-needed",
            "base_url": "http://localhost:11434/v1",
            "comment": "本地模型示例 - 需要先安装 Ollama 并启动服务"
        },
        "groq_fast": {
            "provider": "groq",
            "model": "llama-3.1-70b-versatile",
            "api_key": "your-groq-api-key",
            "base_url": "https://api.groq.com/openai/v1",
            "comment": "Groq 云端加速推理 - 免费额度可用"
        }
    },
    "role_mapping": {
        "WebArchitect": "qwen_max",
        "EmbeddedEngineer": "qwen_max",
        "CodeReviewer": "qwen_turbo",
        "Analyzer": "qwen_max"
    },
    "budget_control": {
        "enabled": true,
        "max_cost_cny": 10.0,
        "max_rounds": 30,
        "warning_threshold": 0.8,
        "comment": "预算控制配置说明",
        "help": {
            "enabled": "是否启用预算控制",
            "max_cost_cny": "最大成本限制（人民币元）",
            "max_rounds": "最大对话轮次",
            "warning_threshold": "警告阈值（0-1），当剩余预算低于此比例时警告"
        }
    },
    "_comment": "配置说明",
    "_help": {
        "provider": "模型提供商: dashscope(阿里云), ollama(本地), groq(云端), openai 等",
        "model": "具体模型名称",
        "api_key": "API 密钥，本地模型可填 'not-needed'",
        "base_url": "API 端点地址"
    }
}